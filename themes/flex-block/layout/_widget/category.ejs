<ul class="wrap-list <%= class_name %>">
  <% page.categories.each(function (categorie) { %>
    <%
      function search(nameKey, targetObj){
        for (var i in targetObj) {
          if (targetObj[i] === nameKey) {
            return i;
          }
        }
      }
      var name = search(categorie.name, config.category_map);
    %>
    <li><a href="<%= url_for(categorie.path) %>">ðŸ“’ <%= name %></a></li>
  <% }) %>
</ul>